{
  "name": "github-card",
  "version": "0.3.3",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:debug": "next build --debug",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "worker:dev": "wrangler dev",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "d1:generate": "drizzle-kit generate",
    "d1:push": "wrangler d1 migrations apply github-card-db",
    "test:kv": "tsx scripts/test-cloudflare-kv.ts",
    "build:analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.9.0",
    "@cloudflare/d1": "^1.0.0",
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@cloudflare/workers-types": "^4.20250506.0",
    "@opennextjs/cloudflare": "^1.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "drizzle-orm": "^0.43.1",
    "framer-motion": "^12.9.4",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.507.0",
    "motion": "^12.9.4",
    "next": "^14.1.0",
    "next-auth": "^4.24.11",
    "pg": "^8.15.6",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.5.6",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/node": "^20",
    "@types/pg": "^8.15.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.32.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "wrangler": "^4.14.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}